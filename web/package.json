    "@types/react-dom": "^18.3.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/three": "^0.171.0",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uglify-js": "^3.17.5",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
