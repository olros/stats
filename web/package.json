{
  "name": "stats",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "pnpm run db:generate && remix vite:build",
    "dev": "remix vite:dev",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc -b",
    "lint": "eslint --fix",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:new-migration": "prisma migrate dev --name",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "docker:down": "docker-compose down",
    "docker:fresh": "pnpm run docker:down && pnpm run docker:start && sleep 1 && pnpm run db:migrate && pnpm run db:generate",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose stop"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource-variable/inter": "^5.0.16",
    "@mui/joy": "^5.0.0-beta.24",
    "@neondatabase/serverless": "^0.9.0",
    "@nivo/bar": "^0.85.0",
    "@nivo/calendar": "^0.85.0",
    "@nivo/core": "^0.85.0",
    "@nivo/heatmap": "^0.85.0",
    "@nivo/line": "^0.85.0",
    "@olros/stats": "^2.2.1",
    "@prisma/adapter-neon": "5.13.0",
    "@prisma/client": "5.13.0",
    "@remix-run/node": "^2.9.1",
    "@remix-run/react": "^2.9.1",
    "@vercel/edge": "^1.1.1",
    "@vercel/remix": "^2.9.0",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.1.0",
    "date-fns": "^3.6.0",
    "is-ip": "^5.0.1",
    "isbot": "^5.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-globe.gl": "^2.27.1",
    "react-intersection-observer": "^9.8.1",
    "react-syntax-highlighter": "^15.5.0",
    "remix-auth": "^3.6.0",
    "remix-auth-github": "^1.7.0",
    "remix-utils": "^7.6.0",
    "slugify": "^1.6.6",
    "three": "^0.163.0",
    "tiny-invariant": "^1.3.1",
    "ua-parser-js": "^1.0.37",
    "uglify-js": "^3.17.4",
    "undici": "^6.14.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.9.1",
    "@remix-run/eslint-config": "^2.9.1",
    "@remix-run/serve": "^2.9.1",
    "@types/d3-scale": "^4.0.8",
    "@types/d3-scale-chromatic": "^3.0.3",
    "@types/node": "^20.12.2",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@types/three": "^0.163.0",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uglify-js": "^3.17.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.4",
    "prisma": "5.13.0",
    "typescript": "^5.4.2",
    "vite": "^5.2.7",
    "vite-tsconfig-paths": "^4.3.1"
  }
}
