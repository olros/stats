{
  "name": "stats",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "yarn db:generate && remix build",
    "dev": "remix dev",
    "start": "remix-serve build/index.js",
    "typecheck": "tsc -b",
    "lint": "eslint --fix src",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:new-migration": "prisma migrate dev --name",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "docker:down": "docker-compose down",
    "docker:fresh": "yarn docker:down && yarn docker:start && sleep 1 && yarn db:migrate && yarn db:generate",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose stop"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource-variable/inter": "^5.0.15",
    "@mui/joy": "^5.0.0-beta.16",
    "@neondatabase/serverless": "^0.6.0",
    "@nivo/bar": "^0.84.0",
    "@nivo/calendar": "^0.84.0",
    "@nivo/core": "^0.84.0",
    "@nivo/heatmap": "^0.84.0",
    "@nivo/line": "^0.84.0",
    "@olros/stats": "^2.2.1",
    "@prisma/adapter-neon": "^5.6.0",
    "@prisma/client": "^5.6.0",
    "@remix-run/node": "^2.3.1",
    "@remix-run/react": "^2.3.1",
    "@vercel/edge": "^1.1.1",
    "@vercel/remix": "^2.2.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.3.1",
    "is-ip": "^5.0.1",
    "isbot": "^3.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-globe.gl": "^2.27.0",
    "remix-auth": "^3.6.0",
    "remix-auth-github": "^1.6.0",
    "remix-utils": "^7.3.0",
    "slugify": "^1.6.6",
    "tiny-invariant": "^1.3.1",
    "ua-parser-js": "^1.0.37",
    "uglify-js": "^3.17.4",
    "undici": "^5.28.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.3.1",
    "@remix-run/eslint-config": "^2.3.1",
    "@remix-run/serve": "^2.3.1",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@types/three": "^0.158.3",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uglify-js": "^3.17.4",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "prisma": "^5.6.0",
    "typescript": "^5.3.2"
  },
  "resolutions": {
    "@remix-run/node": "^2.3.1",
    "@remix-run/server-runtime": "^2.3.1"
  }
}
