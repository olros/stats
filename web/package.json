{
  "name": "stats",
  "description": "The website of Stats",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": "^22"
  },
  "scripts": {
    "prebuild": "pnpm run db:generate",
    "build": "remix vite:build",
    "postinstall": "pnpm run db:generate",
    "dev": "remix vite:dev",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc -b",
    "lint": "eslint --fix",
    "format": "prettier --write .",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:new-migration": "prisma migrate dev --name",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "docker:down": "docker-compose down",
    "docker:fresh": "pnpm run docker:down && pnpm run docker:start && sleep 1 && pnpm run db:migrate && pnpm run db:generate",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose stop"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.1.0",
    "@neondatabase/serverless": "^0.10.4",
    "@nivo/bar": "^0.88.0",
